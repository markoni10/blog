datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x", "debian-openssl-3.0.x"]
  output   = "."
}

model Post {
  id        Int     @id @default(autoincrement())
  title     String
  excerpt   String?
  img       String?
  author_id Int

  @@map("posts")
}

model User {
  id       Int  @id @default(autoincrement())
  username String?
  email    String
  password String

  @@map("users")
}